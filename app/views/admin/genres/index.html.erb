<%= form_with model: @genre, url: admin_genres_path do |f| %>
  <p2>ジャンル名<%= f.text_field :name %></p2>
  <%= f.submit '新規投稿' %>
<% end %>

<table>
  <tr>
    <th>ジャンル</th>
    <th></th>
  </tr>
  <tbody>
    <% @genres.each do |genre| %>
    <tr>
      <td><%= genre.name %></td>
      <td><%= link_to '編集', edit_admin_genre_path(genre.id) %></td>
    </tr>
    <% end %>
  </tbody>
</table>


